<%- include('../partials/html-head') %>
<%- include('../partials/nav') %>
<%- include('../partials/hamburger') %>

<section id="champ-view">
  <div id="champ-index-header">
    <h1 id="table-text-left">Champion</h1>
    <h1 id="table-text-right">Creator</h1>
  </div>
  <div id="champ-table">
    <% champions.forEach(champion => { %>
      <table>
        <tr>
          <th>
            <a id="table-text-left" href="/champions/<%= champion._id %>"><%= champion.name %></a>
            <a id="table-text-right" href="/profiles/<%= champion.owner._id %>"><%= champion.owner.name %></a>
          </th>
        </tr>
      </table>
    <% }) %>
  </div>
</section>

<%- include('../partials/footer') %>